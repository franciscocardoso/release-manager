{
  "name": "release-manager",
  "version": "0.0.0",
  "description": "Utils to manage unconformities between multiple branches on the same repository",
  "homepage": "https://github.com/franciscocardoso/release-manager#readme",
  "bugs": {
    "url": "https://github.com/franciscocardoso/release-manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franciscocardoso/release-manager.git"
  },
  "license": "ISC",
  "author": "Francisco Cardoso <comat8se@gmail.com>",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix bin/* clients/*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged -q"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint"
    ],
    "bin/*": [
      "yarn lint"
    ]
  },
  "dependencies": {
    "@octokit/rest": "^18.5.2",
    "commander": "^7.2.0",
    "lodash": "^4.17.21",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4"
  },
  "bin": {
    "sync-branches": "bin/sync-branches.js"
  }
}
